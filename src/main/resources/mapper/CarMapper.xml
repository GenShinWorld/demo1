<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.CarDao" >
	<!--<cache eviction="FIFO" flushInterval="1000" readOnly="true"></cache>-->
<select id="selectAll" resultType="com.example.demo.entity.Car">
	select * from car where carId=#{carId}
</select>
	<select id="CarImg" resultType="com.example.demo.entity.CarImg">
		select img from carImg
	</select>

</mapper>
